    "next-images": "^1.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-svg": "^15.1.18",
    "swiper": "8.0.7",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.30.0",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
